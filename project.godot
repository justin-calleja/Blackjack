; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "TextureRect",
"class": "Card",
"language": "GDScript",
"path": "res://scenes/card/card.gd"
}, {
"base": "ColorRect",
"class": "Deck",
"language": "GDScript",
"path": "res://scenes/deck/deck.gd"
}, {
"base": "Resource",
"class": "StateMachine",
"language": "GDScript",
"path": "res://addons/fsm/StateMachine.gd"
}, {
"base": "Resource",
"class": "StateMachineFactory",
"language": "GDScript",
"path": "res://addons/fsm/StateMachineFactory.gd"
} ]
_global_script_class_icons={
"Card": "",
"Deck": "",
"StateMachine": "",
"StateMachineFactory": ""
}

[application]

config/name="Blackjack"
run/main_scene="res://scenes/game/game.tscn"
config/icon="res://icon.png"

[display]

window/size/width=720
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "gut" )

[rendering]

environment/default_environment="res://default_env.tres"
